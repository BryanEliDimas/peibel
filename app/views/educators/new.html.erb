<h1>Educator Sign Up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= simple_form_for @educator do |f| %>
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :username %>
      <%= f.input :email %>
      <%= f.input :password %>
      <%= f.input :password_digest, label: "Confirm Password" %>
      <%= f.submit "Sign Up", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
