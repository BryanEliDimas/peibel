<div class="col-md-12">
    <h1>NEW PROJECT</h1>
    <hr>

    <%= simple_form_for @project do |f| %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :content, as: :hidden, id: "project-content" %>

      <div id="project_editor"></div>

      <%= f.submit class: "btn btn-primary" %>
    <% end %>


</div>
